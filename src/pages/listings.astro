---

<MainLayout title="All Listings">
  <div class="min-h-screen bg-gray-50">
    <!-- Hero Section -->
    <div class="bg-gradient-to-r from-teal-600 to-teal-700 text-white py-16">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 text-center">
        <h1 class="text-4xl md:text-5xl font-bold mb-4">All Business Listings</h1>
        <p class="text-xl md:text-2xl opacity-90">Browse our comprehensive collection of business listings across all cities</p>
      </div>
    </div>

    <!-- Main Content -->
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-16">
      <!-- Search and Filter Section -->
      <div class="bg-white rounded-xl shadow-lg p-8 mb-12">
        <h2 class="text-2xl font-bold text-gray-900 mb-6 text-center">Find Your Perfect Business</h2>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
          <!-- Search Input -->
          <div class="relative">
            <label for="searchInput" class="block text-sm font-medium text-gray-700 mb-2">Search Businesses</label>
            <div class="relative">
        <input 
          type="text" 
          id="searchInput"
                placeholder="Search by name, description, or category..." 
                class="w-full px-4 py-3 pl-10 border border-gray-300 rounded-lg focus:ring-4 focus:ring-teal-300 focus:border-transparent transition-all duration-200"
              />
              <svg class="absolute left-3 top-3.5 w-5 h-5 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 0 1 -14 0 7 7 0 0 1 14 0z"></path>
              </svg>
            </div>
          </div>
          
          <!-- City Filter -->
          <div>
            <label for="cityFilter" class="block text-sm font-medium text-gray-700 mb-2">Filter by City</label>
        <select 
          id="cityFilter"
              class="w-full px-4 py-3 border border-gray-300 rounded-lg focus:ring-4 focus:ring-teal-300 focus:border-transparent transition-all duration-200"
        >
          <option value="">All Cities</option>
              {cityOptions.map(city => (
                <option value={city}>{city}</option>
              ))}
        </select>
          </div>
        </div>
        
        <!-- Quick Stats -->
        <div class="mt-8 pt-6 border-t border-gray-200">
          <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 text-center">
            <div>
              <div class="text-2xl font-bold text-teal-600">{listings.length}</div>
              <div class="text-sm text-gray-600">Total Listings</div>
            </div>
            <div>
              <div class="text-2xl font-bold text-teal-600">{cities.length}</div>
              <div class="text-sm text-gray-600">Cities Covered</div>
            </div>
            <div>
              <div class="text-2xl font-bold text-teal-600">{new Set(listings.map(l => l.category)).size}</div>
              <div class="text-sm text-gray-600">Business Categories</div>
            </div>
          </div>
      </div>
    </div>

    <!-- Listings Grid -->
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8" id="listingsGrid">
      {listings.map((listing) => (
          <div class="bg-white rounded-xl shadow-lg hover:shadow-xl transition-all duration-300 transform hover:-translate-y-1">
            <div class="p-6">
              <!-- Listing Image -->
              {listing.main_image ? (
                <div class="w-full h-48 rounded-lg mb-4 overflow-hidden">
                  <img 
                    src={listing.main_image} 
                    alt={listing.name || 'Business Image'} 
                    class="w-full h-full object-cover"
                    loading="lazy"
                  />
                </div>
              ) : (
                <div class="w-full h-48 bg-gray-200 rounded-lg mb-4 flex items-center justify-center">
                  <svg class="w-16 h-16 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
                  </svg>
                </div>
              )}
              
              <!-- Listing Information -->
          <div class="mb-4">
                <h3 class="text-xl font-bold text-gray-900 mb-2">{listing.name}</h3>
                <p class="text-gray-600 text-sm mb-2">{listing.city || 'Location TBD'}, {listing.state || 'CO'}</p>
                <p class="text-gray-700 mb-3 line-clamp-3">{listing.description}</p>
                
                <!-- Category Badge -->
                {listing.category && (
                  <span class="inline-block px-3 py-1 bg-teal-100 text-teal-800 text-xs rounded-full mb-3">
                    {listing.category}
                  </span>
                )}
          </div>
              
              <!-- Contact Information -->
              <div class="space-y-2 mb-4">
                {listing.phone && (
                  <div class="flex items-center text-sm text-gray-600">
                    <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"></path>
                    </svg>
                    {listing.phone}
              </div>
                )}
                {listing.address && (
                  <div class="flex items-center text-sm text-gray-600">
                    <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                    </svg>
                    <span class="truncate">{listing.address}</span>
                  </div>
                )}
              </div>
              
              <!-- Action Button -->
              <button 
                data-listing-id={listing.id}
                class="listing-button w-full bg-teal-600 hover:bg-teal-700 text-white font-medium py-3 px-4 rounded-lg transition-colors shadow-md hover:shadow-lg"
              >
                View Details
              </button>
          </div>
        </div>
      ))}
      </div>
      
      <!-- No Results Message -->
      <div id="noResults" class="hidden text-center py-16">
        <svg class="w-16 h-16 text-gray-400 mx-auto mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.172 16.172a4 4 0 015.656 0M9 12h6m-6-4h6m2 5.291A7.962 7.962 0 0112 15c-2.34 0-4.47-.881-6.08-2.33"></path>
        </svg>
        <h3 class="text-lg font-medium text-gray-900 mb-2">No listings found</h3>
        <p class="text-gray-600">Try adjusting your search criteria or browse all listings.</p>
      </div>
    </div>
  </div>

  <script>
    // Get search parameter from URL and populate search field
    document.addEventListener('DOMContentLoaded', function() {
      const urlParams = new URLSearchParams(window.location.search);
      const searchParam = urlParams.get('search');
    const searchInput = document.getElementById('searchInput');
      
      if (searchParam && searchInput) {
        searchInput.value = decodeURIComponent(searchParam);
        // Trigger filtering with the search term
        filterListings();
      }
    });

    // Search and filter functionality
    function filterListings() {
      const searchTerm = document.getElementById('searchInput').value.toLowerCase();
      const cityFilter = document.getElementById('cityFilter').value;
      const listings = document.querySelectorAll('#listingsGrid > div');
      const noResults = document.getElementById('noResults');
      
      let visibleCount = 0;
      
      listings.forEach(listing => {
        const name = listing.querySelector('h3').textContent.toLowerCase();
        const city = listing.querySelector('p').textContent.toLowerCase();
        const description = listing.querySelector('p:last-of-type').textContent.toLowerCase();
        
        const matchesSearch = name.includes(searchTerm) || city.includes(searchTerm) || description.includes(searchTerm);
        const matchesCity = !cityFilter || city.includes(cityFilter.toLowerCase());
        
        if (matchesSearch && matchesCity) {
          listing.style.display = 'block';
          visibleCount++;
        } else {
          listing.style.display = 'none';
        }
      });
      
      // Show/hide no results message
      if (visibleCount === 0) {
        noResults.classList.remove('hidden');
      } else {
        noResults.classList.add('hidden');
      }
    }
    
    // Add event listeners
    // Add event listeners for real-time filtering
    const searchInputEl = document.getElementById('searchInput');
    const cityFilterEl = document.getElementById('cityFilter');
    
    if (searchInputEl) {
      searchInputEl.addEventListener('input', filterListings);
      // Also add search functionality for Enter key
      searchInputEl.addEventListener('keypress', function(e) {
        if (e.key === 'Enter') {
          filterListings();
        }
      });
    }
    if (cityFilterEl) {
      cityFilterEl.addEventListener('change', filterListings);
    }
    
    // Add event listeners for listing buttons
    const listingButtons = document.querySelectorAll('.listing-button');
    listingButtons.forEach(button => {
      button.addEventListener('click', function() {
        const listingId = this.getAttribute('data-listing-id');
        if (listingId) {
          window.location.href = '/listing/' + listingId;
        }
      });
    });
    
    // Initialize
    filterListings();
  </script>
</MainLayout>